{
  "expo": {
    "name": "iBank-Mobile",
    "slug": "iBank-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/ibank-logo-icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#50c878"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/ibank-logo-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
